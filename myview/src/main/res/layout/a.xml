<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              android:background="#F5F5F5"
              android:orientation="vertical">

    <LinearLayout
        android:id="@+id/menses_mood_top"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="25dp"
        android:layout_marginLeft="25dp"
        android:orientation="horizontal">

        <RelativeLayout
            android:id="@+id/textLayout"
            android:layout_width="80dp"
            android:layout_height="86.66dp"
            android:background="@drawable/mense_mood_select"
            android:orientation="horizontal"
            >

            <ImageView
                android:id="@+id/menses_mood_select"
                android:layout_width="55.55dp"
                android:layout_height="50.5dp"
                android:layout_centerInParent="true"
                android:src="@drawable/seekbar_thumb"
                />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/sssss"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <ImageView
            android:id="@+id/bad"
            android:layout_width="54dp"
            android:layout_height="41dp"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:src="@drawable/ic_launcher"/>

        <LinearLayout
            android:id="@+id/hahha"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="8"
            android:layout_marginRight="9dp"
            android:layout_marginLeft="9dp"
            android:background="@drawable/progress"
            android:gravity="center_vertical"
            >

            <SeekBar
                android:id="@+id/seekbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="true"
                android:thumb="@drawable/aaass"
                android:progressDrawable="@drawable/menses_seekbar_style"
                />
        </LinearLayout>

        <ImageView
            android:id="@+id/happy"
            android:layout_width="54dp"
            android:layout_height="41dp"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:src="@drawable/ic_launcher"/>
    </LinearLayout>

    <Button
        android:id="@+id/testMyButton"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="FUCK"/>
</LinearLayout>


